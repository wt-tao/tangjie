/* pages/result/result.wxss */
.result{width: 100%;height:350rpx;background-color: #FFB500;color: white;}
.title{width: 100%;text-align: center;padding-top: 80rpx;font-size: 28rpx;}
.titles{margin-top: 30rpx;font-size: 80rpx;font-weight: bold;width: 100%;text-align: center;}
.div{width: 94%;margin-left: 3%;}
.div_top{line-height: 100rpx;width: 100%;font-size: 26rpx;color: #3C3C3C;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.div_icon{width: 12rpx;height: 50rpx;margin-top: 25rpx;background-color: #FF8AA6}
.div_img{width: 100%;height: 250rpx;border-radius: 15rpx;border: 1rpx solid black;}
.div_title{line-height: 80rpx;color: #333333;font-size: 32rpx;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.div_count{line-height: 60rpx;font-size: 26rpx;color: #A3A3A3;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.div_price{width: 100%;height: 100rpx;line-height: 100rpx;}
.num_day{width: 230rpx;text-align: center;height:60rpx;line-height: 60rpx;margin-top: 20rpx;background-color: #FF85A2;color: white;font-size: 28rpx;border-radius: 5rpx;}
.num_money{width: 60%;color: #FF2717;font-size: 30rpx;text-align: right;margin-left:50rpx;}